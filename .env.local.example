# ===========================================
# MRBRICO IMMO - Variables d'environnement
# ===========================================
# Copie ce fichier vers .env.local et remplis les valeurs

# Supabase
# Obtenir depuis: https://supabase.com/dashboard/project/[project-id]/settings/api
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Trello (optionnel pour Phase 1)
# Obtenir depuis: https://trello.com/power-ups/admin
TRELLO_API_KEY=
TRELLO_TOKEN=
TRELLO_BOARD_ID=

# SendGrid (optionnel pour Phase 1)
# Obtenir depuis: https://app.sendgrid.com/settings/api_keys
SENDGRID_API_KEY=
SENDGRID_FROM_EMAIL=info@mrbrico.ca

# App
NEXT_PUBLIC_APP_URL=http://localhost:3000
NEXT_PUBLIC_MAIN_SITE_URL=https://mrbrico.ca
